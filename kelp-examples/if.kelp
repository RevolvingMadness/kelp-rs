mcfn namespace:main {
    let value = 3 to score

    if (value == 1) {
        tellraw @a "value == 1"
    } else if (value == 2) {
        tellraw @a "value == 2"
    } else {
        tellraw @a "value != 1 && value != 2 #1" // Runs
    }

    if (value != 1 && value != 2) {
        tellraw @a "value != 1 && value != 2 #2" // Runs
    }
}
