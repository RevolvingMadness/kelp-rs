mcfunction namespace:main {
    let value: score = 3

    if (value == 1) {
        tellraw @a "value == 1"
    } else if (value == 2) {
        tellraw @a "value == 2"
    } else {
        tellraw @a "value != 1 && value != 2 #1"
    }

    if (value != 1 && value != 2) {
        tellraw @a "value != 1 && value != 2 #2"
    }
}
