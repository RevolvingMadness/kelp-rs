mcfn namespace:main {
    let counts = {
        a: 3,
        b: 1,
        c: 4
    } to data

    for key in ["a", "c", "d"] {
        let entry = &counts.get(key)

        if !entry {
            tellraw @a [key, " -> nothing"]
        } else {
            tellraw @a [key, " -> ", entry]
        }
    }
}